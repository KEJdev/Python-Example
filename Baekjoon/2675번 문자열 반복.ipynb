    "for _ in range(cnt):\n",
    "    temp = list(map(str, input().split(' ')))\n",
    "    result = []\n",
    "    for val in temp[1]:\n",
    "        result.append(int(temp[0])*val)\n",
    "    print(\"\".join(result))"
