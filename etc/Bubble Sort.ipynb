   "source": [
    "num = [5,1,2,7,3,8]\n",
    "\n",
    "def bubble_sort(num):\n",
    "    for _ in range(len(num)-1, 0, -1):\n",
    "        for idx in range(len(num)-1):\n",
    "            if num[idx] > num[idx+1]:\n",
    "                num[idx], num[idx+1] = num[idx+1], num[idx]\n",
    "    return num\n",
    "print(bubble_sort(num))"
   ]
  }
