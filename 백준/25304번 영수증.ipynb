   "source": [
    "result_money = int(input())\n",
    "cnt = int(input())\n",
    "result = 0\n",
    "for val in range(cnt):\n",
    "    money, mul = list(map(int, input().split(' ')))\n",
    "    result+=money*mul \n",
    "\n",
    "if result == result_money:\n",
    "    print('Yes')\n",
    "else:\n",
    "    print('No')"
   ]
