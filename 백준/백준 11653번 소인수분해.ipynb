   "source": [
    "result = []\n",
    "k=2\n",
    "while N:\n",
    "    if N==1:\n",
    "        result.append(N)\n",
    "        break\n",
    "        \n",
    "    elif N%k == 0:\n",
    "        N= N//k\n",
    "        result.append(N)\n",
    "    else:\n",
    "        k+=1\n",
    "print(result)\n",
    "        "
